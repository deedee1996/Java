package com.example.demo;

import org.springframework.context.annotation.Scope;

@Scope("singleton")
public class Product {

	private String Name;

	public Product(String name) {
		super();
		Name = name;
	}

	 public String getName() {
		return Name;
	}

	 public void setName(String name) {
		Name = name;
	}
	
}
